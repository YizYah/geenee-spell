{
  "name": "geenee-spell",
  "version": "0.0.18",
  "description": "regenerates project code based upon settings and a template",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "ava",
    "posttest": "npm run view-coverage && npm run report && npm run lint",
    "prepack": "rm -rf lib && tsc -b",
    "report": "nyc report --reporter=json",
    "view-coverage": "nyc --extension .ts ava --forbid-only"
  },
  "keywords": [
    "ts-packrat"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "custom-jeans": "0.0.5",
    "dynamapping": "^1.6.0",
    "fs-extra": "^9.0.1",
    "geenee-rate": "^0.1.7",
    "head-starter": "^1.1.5",
    "inquirer": "^7.3.3",
    "magicalstrings": "0.0.14",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "mock-fs": "^4.13.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "bugs": "https://github.com/YizYah/geenee-spell/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/geenee-spell",
  "repository": "YizYah/geenee-spell",
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
