{
  "name": "geenee-spell",
  "version": "0.0.10",
  "description": "regenerates project code based upon settings and a template",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "ava",
    "posttest": "eslint \"src/**/*.ts\"",
    "prepack": "rm -rf lib && tsc -b"
  },
  "keywords": [
    "ts-packrat"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "custom-jeans": "0.0.2",
    "fs-extra": "^9.0.1",
    "geenee-rate": "^0.0.4",
    "inquirer": "^7.3.3",
    "magicalstrings": "0.0.13",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "bugs": "https://github.com/YizYah/geenee-spell/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/geenee-spell",
  "repository": "YizYah/geenee-spell",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
